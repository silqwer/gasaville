<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <script src='/common/js/jquery/jquery.min.js'></script>
    <script src='/adm/js/gosa.js'></script>
    <link rel='stylesheet' href='/adm/css/style.css' />
  </head>
  <body>
  	<input type="hidden" id="examPage" name="examPage" value="<%=page%>">
    <h1><%= title %></h1>
    <div>
    	<select>
		  <option value="name" selected="selected">고사장</option>
		  <option value="school">학교명</option>
		  <option value="period">기수</option>
		  <option value="department">소속</option>
		  <option value="position">직책</option>
		  <option value="name">이름</option>
		</select>
    </div>
    <div><input type="text" id="searchWord" name="searchWord"><button>검색</button></div>
   	<table>
   		<tr>
   			<th>번호</th>
   			<th>고사장</th>
   			<th>학교</th>
   			<th>기수</th>
			<th>소속</th>
   			<th>직책</th>
   			<th>이름</th>		
   		</tr>
   		<% for(let i=0; i<list.length; ++i){ %>
   		<tr>
   			<td><%=max-i%></td>
   			<td><%=list[i].EXAM_NAME%></td>
   			<td><%=list[i].EXAM_SCHOOL%></td>
   			<td><%=list[i].SCHEDULE_NAME%></td>
   			<td><%=list[i].SCHEDULE_NAME%></td>
   			<td><%=list[i].POSITION%></td>
   			<td><%=list[i].USER_NAME%></td>
   		</tr>
   		<% } %>
   	</table>
   	<div>
      <% if(page > pageSize) { %>
        [ <a href="/admin/exam/history/list/1" style="text-decoration: none;">◀◀</a> ]
        [ <a href="/admin/exam/history/list/<%=startPage-1%>" style="text-decoration: none;">◀</a> ]
      <% } %>
     
      &nbsp;&nbsp;
      <% for (var i=startPage; i<=endPage; i++) { %>
        <% if (i== page) {%>
          [ <%=i%> ]
        <% } else { %>
          [ <a href="/admin/exam/history/list/<%=i%>" style="text-decoration: none;"><%=i%></a> ]
        <% } %>
      <% } %>
      &nbsp;&nbsp;
      <% if (endPage < totalPage) { %>
        [ <a href="/admin/exam/history/list/<%=endPage+1%>" style="text-decoration: none;">▶</a> ]
        [ <a href="/admin/exam/history/list/<%=totalPage%>" style="text-decoration: none;">▶▶</a> ]
      <% } %>
      
    </div>
    <div>
    </div>
  </body>
</html>
