<!DOCTYPE html>
<html>
	<head>
    <title><%= title %></title>
   	<% include ../common/head %>
  </head>
  <body>
   	<div class="main-wrap">
    	<% include ../common/menu %>
    	<div class="content-wrap">
    		<h2><%= title %></h2>
    		<form id="commonForm" name="commonForm" enctype="multipart/form-data" action="/admin/periods/excel/upload" method="post">
    
	    		<!-- 스케줄 리스트 -->
	    		<select id="scheduleCategory" name="scheduleCategory" style="margin-right: 5px;">
	    		<option value="null">일정을 선택해주세요.</option>
	    		<% for(let i=0; i<schedule.length; ++i){ %>
	    	  	<option value="<%=schedule[i].SEQ%>"><%=schedule[i].NAME%></option>
	    		<%}%>
					</select>
	    		
	    		<input type="file" id="excelFile" name="excel">
				</form>
 
				<div class="btnfr">
					<button type="button" id="sendBtn" class="mbtn skybtn">등록</button>
				</div>
    	</div>
    </div>
    
  </body>
  
  <script src='/adm/js/periods.js'></script>
</html>
