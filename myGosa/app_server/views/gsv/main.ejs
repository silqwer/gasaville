				<% let row_span_flag = true; %>
				<% let rowsp = 0; %>
				<% let temp = 0; %>
				<% let switch_flag = true; %>
				<% let index_key = 0; %>

				<% result.forEach(function(el, index) { %>
				<% if(locals.result[index+1] && el['EXAM_SEQ'] === result[index+1]['EXAM_SEQ'] && rowsp === 0) { %>
				<% switch_flag = false; %>
				<% row_span_flag = false; %>
				<% temp = index; %>
				<% 	while(true) { %>
				<% 		if(el['EXAM_SEQ'] === result[temp]['EXAM_SEQ']) { %>
				<%			temp = temp+1; %>
				<%			rowsp = rowsp+1; %>
				<%		} else { %>
				<%			break;%>
				<%		} %>
				<%  } %>
				<% } else { %>
				<% 		if(switch_flag === false) { %>
				<%			rowsp = rowsp-1; %>
				<%			if(rowsp === 0) { %>
				<%				switch_flag = true; %>
				<% 			} %>
				<% 		} %>
				<%		row_span_flag = true; %>
				<% } %> 
				<tr>
				<%	if(row_span_flag === true) { %>
				<%		if(switch_flag === true) { %>
						<td class="tac"><%=index_key%></td>
						<% index_key++; %>
						<td class="tac"><%=el['EXAM_SCHOOL'];%></td>
						<td class="tac"><%=el['EXAM_NAME'];%></td>
						<td class="tac"><%=el['PERIOD_CLASS'];%></td>
				<% 		} %>
				<% } else { %>
						<td rowspan="<%=rowsp%>" class="tac"><%=index_key;%></td>
						<% index_key++; %>
						<td rowspan="<%=rowsp%>" class="tac"><%=el['EXAM_SCHOOL'];%></td>
						<td rowspan="<%=rowsp%>" class="tac"><%=el['EXAM_NAME'];%></td>
						<td rowspan="<%=rowsp%>" class="tac"><%=rowsp;%></td>
				<% } %>
				<td class="tac"><%=el['PERIOD_CLASS'];%></td>
				<td class="tac">
				<% if(applyStatus === 0) { %>
				<button type="button" class="mbtn skybtn readonly" readonly="readonly">신청대기</button>
				<% } else { %>
				<% switch(el['USER_STATE']) {
					case 0: %>
				<button type="button" class="mbtn skybtn applyBtn" 
						data-period="<%=el.SEQ%>"
						data-schedule="<%=schedule[0].SEQ%>"
						data-class="<%=el.PERIOD_CLASS%>"
				>신청하기</button>
				<% break;
					case 1: %>
